{
  "name": "intlit-vue",
  "version": "0.1.0",
  "description": "Vue plugin and composables for intlit.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint src --ext .ts,.vue",
    "format": "prettier --write \"src/**/*.{ts,vue}\""
  },
  "keywords": [
    "intl",
    "formatter",
    "i18n",
    "vue",
    "vue-composable"
  ],
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "intlit-core": "file:../intlit-core"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "jsdom": "^26.1.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue": "^3.4.21"
  }
}
